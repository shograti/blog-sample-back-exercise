{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}

<h1>Articles</h1>
<ul>
{% for article in articles %}
        <h2>{{article.titre}}</h2>
        <li>{{ article.contenu }}</li>
    {% endfor %}
</ul>

<h1>Utilisateurs</h1>

{% for utilisateur in utilisateurs %}

        <li>{{ utilisateur.pseudo }}</li>
    {% endfor %}


{% endblock %}